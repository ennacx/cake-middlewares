{
    "name": "ennacx/cake-middlewares",
    "description": "Middlewares for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": "^8.1",
        "cakephp/cakephp": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Ennacx\\CakeMiddlewares\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ennacx\\CakeMiddlewares\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
